                            {
                                "Id": "NotableQueriesBuilderBlade",
                                "Type": "Blade",
                                "Version": 0,
                                "Configuration": {
                                    "General": {
                                        "title": "",
                                        "newGroup": false,
                                        "preselectedFilters": "",
                                        "renderMode": "grid"
                                    },
                                    "queries": [
                                        {
                                            "query": "Type=log_regionserver_CL |select  Message",
                                            "displayName": "HBase Region Server Logs"
                                        },
                                        {
                                            "query": "Type=log_restserver_CL |select  Message",
                                            "displayName": "HBase REST Server Logs"
                                        },
                                        {
                                            "query": "Type=log_phoenixserver_CL  |select  Message",
                                            "displayName": "PhoenixLogs"
                                        },
                                        {
                                            "query": "Type=log_hmaster_CL |select  Message",
                                            "displayName": "HMaster Logs"
                                        },
                                        {
                                            "query": "\"Exception\" |Measure Count() by ClusterName_s interval 10minute",
                                            "displayName": "Exceptions over Time"
                                        },
                                        {
                                            "query": "\"Error\" |Measure Count() by ClusterName_s interval 10minute",
                                            "displayName": "Errors over time"
                                        },
                                        {
                                            "query": "\"WARN\" |Measure Count() by ClusterName_s interval 10minute",
                                            "displayName": "WARN messages over time"
                                        },
                                        {
                                            "query": "\"Failed\" |Measure Count() by ClusterName_s interval 10minute",
                                            "displayName": "Failed messages over time"
                                        },
                                        {
                                            "query": "(Type=log_resourcemanager_CL)| select Message",
                                            "displayName": "Yarn Resource Manager Logs"
                                        },
                                        {
                                            "query": "* (Type=log_timelineserver_CL)|select Message",
                                            "displayName": "Yarn Timeline server Logs"
                                        }
                                    ]
                                }
                            },
                            {
                                "Id": "NotableQueriesBuilderBlade",
                                "Type": "Blade",
                                "Version": 0,
                                "Configuration": {
                                    "General": {
                                        "title": "Popular HBase Charts",
                                        "newGroup": false,
                                        "preselectedFilters": "Type, Computer",
                                        "renderMode": "default"
                                    },
                                    "queries": [
                                        {
                                            "query": "Type= metrics_regionserver_CL | measure  avg(writeRequestCount_d), min(writeRequestCount_d), max(writeRequestCount_d) ,pct95(writeRequestCount_d) by ClusterName_s, HostName_s",
                                            "displayName": "Avg, Min , Max Writes"
                                        },
                                        {
                                            "query": " Type=  metrics_regionserver_os_CL  | measure avg(CommittedVirtualMemorySize_d) by ObjectName_s interval 1minute",
                                            "displayName": "Committed Virtual Memory Size"
                                        },
                                        {
                                            "query": "Type=  metrics_regionserver_os_CL | measure avg(TotalPhysicalMemorySize_d), avg(FreePhysicalMemorySize_d) by ObjectName_s interval 1minute",
                                            "displayName": "Free vs Total Memory"
                                        },
                                        {
                                            "query": "Type=  metrics_regionserver_os_CL | measure avg(OpenFileDescriptorCount_d), avg(MaxFileDescriptorCount_d) by ObjectName_s interval 1minute",
                                            "displayName": "Open Files vs Total Open Files"
                                        },
                                        {
                                            "query": "Type= metrics_hmaster_jvm_CL  | measure avg(ThreadsWaiting_d) by HostName_s ",
                                            "displayName": "HMaster JVM Thread Waiting"
                                        },
                                        {
                                            "query": "Type= metrics_hmaster_CL |measure avg(numDeadRegionServers_d) by HostName_s ",
                                            "displayName": "Dead Region Server"
                                        },
                                        {
                                            "query": " Type=metrics_regionserver_os_CL  | measure avg(SystemLoadAverage_d) by ObjectName_s interval 1minute",
                                            "displayName": "Average Load"
                                        },
                                        {
                                            "query": "Type=metrics_regionserver_os_CL | measure avg(SystemCpuLoad_d) by ObjectName_s interval 1minute",
                                            "displayName": "CPU Load"
                                        },
                                        {
                                            "query": "Type= metrics_hmaster_ipc_CL| measure avg(sentBytes_d) by HostName_s ",
                                            "displayName": "HMaster IPC sentBytes"
                                        },
                                        {
                                            "query": "Type= metrics_hmaster_ipc_CL| measure avg( receivedBytes_d) by HostName_s ",
                                            "displayName": "HMaster IPC receivedBytes"
                                        },
                                        {
                                            "query": "(Type=metrics_regionserver_ipc_CL)|measure avg(exceptions_d) by ClusterName_s interval 10minute",
                                            "displayName": "Exceptions Over Time"
                                        },
                                        {
                                            "query": "(Type=metrics_regionserver_wal_CL) |measure avg(AppendSize_95th_percentile_d) by HostName_s interval 1hour",
                                            "displayName": "WAL Append Size"
                                        },
                                        {
                                            "query": "(Type=metrics_regionserver_wal_CL) |measure avg(SyncTime_95th_percentile_d) by HostName_s interval 1hour",
                                            "displayName": "WAL Sync time"
                                        },
                                        {
                                            "query": "(Type=metrics_regionserver_jvm_CL)|measure avg(GcTimeMillis_d) by ClusterName_s, HostName_s interval 10minute",
                                            "displayName": "Region Server GC Time"
                                        }
                                    ]
                                }
                            },
